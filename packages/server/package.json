{
    "name": "server",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon",
        "build": "webpack",
        "start:prod": "NODE_ENV=production node server",
        "db:migrate": "rm -rf prisma/migrations; npx prisma migrate dev",
        "db:browse": "npx prisma studio",
        "db:clear": "npx prisma migrate reset",
        "db:clear-not-available": "DISABLE_SYNC=true node -e 'require(\"./jobs\").clearNotAvailable()'"
    },
    "prisma": {
        "schema": "prisma/schema.prisma"
    },
    "engines": {
        "node": ">= 18.0.0",
        "yarn": ">= 1.22.19"
    },
    "dependencies": {
        "@prisma/client": "^4.11.0",
        "axios": "^1.4.0",
        "body-parser": "^1.20.2",
        "chokidar": "^3.5.3",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "figlet": "^1.5.2",
        "get-audio-duration": "^3.1.1",
        "helmet": "^7.0.0",
        "qs": "^6.11.0",
        "universal-cookie": "^4.0.4"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/body-parser": "^1.19.2",
        "@types/chokidar": "^2.1.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/figlet": "^1.5.6",
        "@types/node": "^20.4.5",
        "dotenv": "^16.0.3",
        "nodemon": "^3.0.1",
        "prisma": "^4.11.0",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    }
}
